package com.beepeers.common.ro;

import java.util.List;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class RecommendationRO {
	
	public enum SocialNetworkRecoRO {
		FACEBOOK
	}
	
	private Long target;
	
	private List<Long> receivers;
	
	private List<SocialNetworkRecoRO> sonetrec;

	public Long getTarget() {
		return target;
	}

	public void setTarget(Long target) {
		this.target = target;
	}

	public List<Long> getReceivers() {
		return receivers;
	}

	public void setReceivers(List<Long> receivers) {
		this.receivers = receivers;
	}

	public List<SocialNetworkRecoRO> getSonetrec() {
		return sonetrec;
	}

	public void setSonetrec(List<SocialNetworkRecoRO> sonetrec) {
		this.sonetrec = sonetrec;
	}

}
