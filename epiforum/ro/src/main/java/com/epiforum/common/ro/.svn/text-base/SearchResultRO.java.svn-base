package com.beepeers.common.ro;

import java.util.List;

public class SearchResultRO {
	
	private List<ProfileRO> companies;
	
	private int startIndex;
	
	private int endIndex;
	
	private int nbResult;
	
	private List<SearchCategoryRO> categories;

	public List<ProfileRO> getCompanies() {
		return companies;
	}

	public void setCompanies(List<ProfileRO> companies) {
		this.companies = companies;
	}

	public int getStartIndex() {
		return startIndex;
	}

	public void setStartIndex(int startIndex) {
		this.startIndex = startIndex;
	}

	public int getEndIndex() {
		return endIndex;
	}

	public void setEndIndex(int endIndex) {
		this.endIndex = endIndex;
	}

	public int getNbResult() {
		return nbResult;
	}

	public void setNbResult(int nbResult) {
		this.nbResult = nbResult;
	}

	public List<SearchCategoryRO> getCategories() {
		return categories;
	}

	public void setCategories(List<SearchCategoryRO> categories) {
		this.categories = categories;
	}

}
