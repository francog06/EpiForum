package com.beepeers.common.ro;

import java.util.ArrayList;
import java.util.List;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class ProfileListSectionRO {
	private String name;
	private int orderNum;
	private List<ProfileListItemRO> items;
	
	public ProfileListSectionRO() {
		this.items = new ArrayList<ProfileListItemRO>();
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getOrderNum() {
		return orderNum;
	}

	public void setOrderNum(int orderNum) {
		this.orderNum = orderNum;
	}

	public List<ProfileListItemRO> getItems() {
		return items;
	}

	public void setItems(List<ProfileListItemRO> items) {
		this.items = items;
	}

}
