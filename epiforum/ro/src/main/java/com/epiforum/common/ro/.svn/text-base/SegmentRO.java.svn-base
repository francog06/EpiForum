package com.beepeers.common.ro;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class SegmentRO {

	private int id;

	private String label;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getLabel() {
		return label;
	}

	public void setLabel(String label) {
		this.label = label;
	}

}